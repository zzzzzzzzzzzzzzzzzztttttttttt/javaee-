<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hqyj.mapper.EmployeeFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hqyj.pojo.EmployeeFile">
        <id column="file_id" property="fileId" />
        <result column="employee_id" property="employeeId" />
        <result column="file_name" property="fileName" />
        <result column="content_summary" property="contentSummary" />
        <result column="employee_name" property="employeeName" />
        <result column="archive_type" property="archiveType" />
        <result column="is_delete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        file_id, employee_id, file_name, content_summary, employee_name, archive_type, is_delete
    </sql>

</mapper>
